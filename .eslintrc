{
    "extends": ["airbnb/base", "plugin:react/recommended"],
    "env": {
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "jsx-a11y",
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-plusplus": 0,
        "comma-dangle": 0,
        "indent": ["error", 4],
        "no-unresolved": 0,
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "global-require": 0,
        "no-nested-ternary": 0,
        "new-cap": ["error", { "capIsNewExceptions": ["Router"] }],
        "class-methods-use-this": 0
    }
}
